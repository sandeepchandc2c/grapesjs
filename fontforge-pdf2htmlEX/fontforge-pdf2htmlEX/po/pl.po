msgid "Navigation"
msgstr "Nawigacja"

#: ../fontforgeexe/prefs.c:551
msgid "Editing"
msgstr "Edycja"

#: ../fontforgeexe/prefs.c:552
msgid "Synchronize"
msgstr "Synchronizuj"

